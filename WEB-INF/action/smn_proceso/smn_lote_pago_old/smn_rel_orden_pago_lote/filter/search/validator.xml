<?xml version='1.0' encoding='ISO-8859-1'?>
<validator id='filter.params' onerror='/action/error/validation/ajax'>
	<parameter id='smn_orden_pago_id' type='integer' required='false' label='${lbl:b_smn_orden_pago_id}' maxlen='10'/>
	<parameter id='smn_lote_pago_id' type='integer' required='false' label='${lbl:b_smn_lote_pago_id}' maxlen='10'/>
	<parameter id='smn_documento_id' type='integer' required='false' label='${lbl:b_smn_documento_id}' maxlen='10'/>
	<parameter id='eol_numero_pago' type='integer' required='false' label='${lbl:b_eol_numero_pago}' maxlen='10'/>
	<parameter id='eol_estatus' type='varchar' required='false' label='${lbl:eol_estatus}' maxlen='2'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='eol_estatus' rule='like' />
	<parameter id='eol_fecha_registro_init' type='date' required='false' label='${lbl:b_store_date}' maxlen='10'/>
	<custom-validator classname='dinamica.validators.DateRangeValidator' on-error-label='[${lbl:b_store_date}@{${def:date}} mayor o igual que] no puede ser mayor que [${lbl:b_store_date}@{${def:date}} menor o igual que].' date1='eol_fecha_registro_init' date2='eol_fecha_registro_end' id='eol_fecha_registro_init' />
	<parameter id='eol_fecha_registro_end' type='date' required='false' label='${lbl:b_store_date}' maxlen='10'/>
</validator>
