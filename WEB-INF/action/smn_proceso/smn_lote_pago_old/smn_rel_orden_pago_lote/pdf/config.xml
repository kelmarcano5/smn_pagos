<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Generar reporte generico PDF
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnrelordenpagolote.sql" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>dinamica.PDFGenericReport</classname>
	</output>
	
	<pdf-title>${lbl:b_smn_rel_orden_pago_lote_id}</pdf-title>
	<pdf-logo url='/images/logo-simone-pdf.png' scale='100' />
	<pdf-file-name>${lbl:b_smn_rel_orden_pago_lote_id}.pdf</pdf-file-name>

	<pdf-report 
		pageSize="letter" 
		rotate="false"
		footer="false" 
		header="true">
		<record recordset='query-smnrelordenpagolote.sql' width='100' spacingBefore='40'>
			<col name='smn_orden_pago_id_combo' title='${lbl:b_smn_orden_pago_id}' width='8' align='left' format='######' />
			<col name='smn_lote_pago_id_combo' title='${lbl:b_smn_lote_pago_id}' width='8' align='left' format='######' />
			<col name='smn_documento_id_combo' title='${lbl:b_smn_documento_id}' width='8' align='left' format='######' />
			<col name='eol_numero_pago' title='${lbl:b_eol_numero_pago}' width='8' align='left' format='######' />
			<col name='eol_fecha_pago' title='${lbl:b_eol_fecha_pago}' width='8' align='left' format='dd-MM-yyyy' />
			<col name='eol_referencia_bancaria' title='${lbl:b_eol_referencia_bancaria}' width='8' align='left' />
			<col name='eol_monto_pago_ml' title='${lbl:b_eol_monto_pago_ml}' width='8' align='left' format='#,###,##0.00' />
			<col name='smn_moneda_rf_combo' title='${lbl:b_smn_moneda_rf}' width='8' align='left' format='######' />
			<col name='smn_tasa_cambio_rf_combo' title='${lbl:b_smn_tasa_cambio_rf}' width='8' align='left' format='######' />
			<col name='eol_monto_pago_ma' title='${lbl:b_eol_monto_pago_ma}' width='8' align='left' format='#,###,##0.00' />
			<col name='eol_estatus_combo' title='${lbl:eol_estatus}' width='8' align='left' />
			<col name='eol_fecha_registro' title='${lbl:b_store_date}' width='8' align='left' format='dd-MM-yyyy' />

		</record>

	</pdf-report>
</config>
