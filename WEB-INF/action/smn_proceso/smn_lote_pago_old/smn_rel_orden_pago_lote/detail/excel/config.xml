<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query.sql" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>domain.ExcelReportOutput</classname>
	</output>
	
	<excel-logo scale="60" url="/images/logo-simone-pdf.png"/>
	<title cellPosY="1" cellPosX="6" cellStep="0" name="${lbl:b_smn_rel_orden_pago_lote_id}"/>
	
	<excel 
		recordset='query.sql' 
		sheetname='${lbl:b_smn_rel_orden_pago_lote_id}' 
		filename='${lbl:b_smn_rel_orden_pago_lote_id}.xls' 
		date-format='dd-MM-yyyy'
		header-background='true'>
		<col id='smn_orden_pago_id_combo' label='${lbl:b_smn_orden_pago_id}' />
		<col id='smn_lote_pago_id_combo' label='${lbl:b_smn_lote_pago_id}' />
		<col id='smn_documento_id_combo' label='${lbl:b_smn_documento_id}' />
		<col id='eol_numero_pago' label='${lbl:b_eol_numero_pago}' />
		<col id='eol_fecha_pago' label='${lbl:b_eol_fecha_pago}' />
		<col id='eol_referencia_bancaria' label='${lbl:b_eol_referencia_bancaria}' />
		<col id='eol_monto_pago_ml' label='${lbl:b_eol_monto_pago_ml}' />
		<col id='smn_moneda_rf_combo' label='${lbl:b_smn_moneda_rf}' />
		<col id='smn_tasa_cambio_rf_combo' label='${lbl:b_smn_tasa_cambio_rf}' />
		<col id='eol_monto_pago_ma' label='${lbl:b_eol_monto_pago_ma}' />
		<col id='eol_estatus_combo' label='${lbl:eol_estatus}' />
		<col id='eol_fecha_registro' label='${lbl:b_store_date}' />

	</excel>

</config>
