<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnlotepago.sql" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>domain.ExcelReportOutput</classname>
	</output>
	
	<excel-logo scale="60" url="/images/logo-simone-pdf.png"/>
	<title cellPosY="1" cellPosX="5" cellStep="0" name="${lbl:b_smn_lote_pago_id}"/>
	
	<excel 
		recordset='query-smnlotepago.sql' 
		sheetname='${lbl:b_smn_lote_pago_id}' 
		filename='${lbl:b_smn_lote_pago_id}.xls' 
		date-format='dd-MM-yyyy'
		header-background='true'>
		<col id='smn_entidad_financiera_rf_combo' label='${lbl:b_smn_entidad_financiera_rf}' />
		<col id='smn_cuenta_bancaria_rf_combo' label='${lbl:b_smn_cuenta_bancaria_rf}' />
		<col id='smn_documento_id_combo' label='${lbl:b_smn_documento_id}' />
		<col id='lpa_numero_lote' label='${lbl:b_lpa_numero_lote}' />
		<col id='lpa_descripcion_lote' label='${lbl:b_lpa_descripcion_lote}' />
		<col id='lpa_fecha_lote' label='${lbl:b_lpa_fecha_lote}' />
		<col id='smn_forma_pago_rf_combo' label='${lbl:b_smn_forma_pago_rf}' />
		<col id='lpa_monto_lote_ma' label='${lbl:b_lpa_monto_lote_ma}' />
		<col id='lpa_monto_lote_ml' label='${lbl:b_lpa_monto_lote_ml}' />
		<col id='lpa_estatus_combo' label='${lbl:b_lpa_estatus}' />
		<col id='lpa_fecha_registro' label='${lbl:b_store_date}' />

	</excel>

</config>
