<?xml version='1.0' encoding='ISO-8859-1'?>
<config>

	<summary>
		${lbl:b_reception_note}
	</summary>

	<log>false</log>
	
	<set-request-attribute id="progress" value="file:/WEB-INF/action/progress/progress.htm"/>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id='query-smn_orden_compra_rf.sql' source='sql' scope='transaction'/><recordset id='query-smn_documento_id.sql' source='sql' scope='transaction'/><recordset id='query-smn_centro_costo_rf.sql' source='sql' scope='transaction'/><recordset id='query-smn_proveedor_id.sql' source='sql' scope='transaction'/><recordset id='query-smn_usuario_rf.sql' source='sql' scope='transaction'/>
		<recordset id="notarecepcion" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>dinamica.GenericOutput</classname>
		<template>template.htm</template>
		<set-http-headers>true</set-http-headers>
		<content-type>text/html</content-type>
		<print mode='table' recordset='query-smn_orden_compra_rf.sql' tag='smn_orden_compra_rf_rows'/><print mode='table' recordset='query-smn_documento_id.sql' tag='smn_documento_id_rows'/><print mode='table' recordset='query-smn_centro_costo_rf.sql' tag='smn_centro_costo_rf_rows'/><print mode='table' recordset='query-smn_proveedor_id.sql' tag='smn_proveedor_id_rows'/><print mode='table' recordset='query-smn_usuario_rf.sql' tag='smn_usuario_rf_rows'/>
		<print mode="form" recordset="notarecepcion" />
	</output>

</config>
