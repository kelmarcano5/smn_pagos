<?xml version='1.0' encoding='ISO-8859-1'?>
<validator maxErrors='20'>
	<parameter id='foi_codigo' type='varchar' required='true' maxlen='16'/>
	<parameter id='foi_descripcion' type='varchar' required='true' maxlen='64'/>
	<parameter id='foi_texto' type='varchar' required='true' maxlen='10'/>
	<parameter id='foi_estatus' type='varchar' required='true' maxlen='2'/>
	<parameter id='foi_vigencia' type='date' required='true' maxlen='10'/>


	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_not_duplicated}.'
			sql='buscarCodigo.sql'
			id='foi_codigo'/>

	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_not_duplicated}.'
			sql='buscarDescripcion.sql'
			id='foi_descripcion'/>


</validator>
